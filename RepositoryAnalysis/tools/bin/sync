#!/usr/bin/env perl

use strict;
use warnings;

use FindBin;
use lib "$FindBin::RealBin/../lib";
use CRKN::Repository::sync;
use Getopt::Long;


my $conf = "/etc/canadiana/tdr/tdr.conf";

GetOptions (
    'conf:s' => \$conf
    );

CRKN::Repository::sync->new({ 
    configpath => $conf
                            })->sync;

1;
