#!/usr/bin/env perl

use strict;
use warnings;

use FindBin;
use lib "$FindBin::RealBin/../lib";
use CRKN::Repository::Loadfilemeta;
use Getopt::Long;


my $conf = "/etc/canadiana/tdr/tdr.conf";

GetOptions (
    'conf:s' => \$conf
    );

CRKN::Repository::Loadfilemeta->new({ 
    configpath => $conf
                            })->load;

1;
